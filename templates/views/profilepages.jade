extends ../layouts/default

block content
  .project-categories
    ul.list-unstyled.content
      each profilePage in profilePages
        li: a(href= '/profile/' + profilePage.title.toLowerCase())
          img(src= '/images/profilepages/thumb/' + profilePage.image.filename)
          h4.capitalize= profilePage.title

block sidebar
  .sidebar
    h2 Profile
    h3.mja-breadcrumb
      a(href='/') Home
      em  - Profile
    !=page.sidebarContent
